{
  "name": "scopeli/ux-bpmn",
  "type": "symfony-bundle",
  "description": "BPMN viewer and modeler for Symfony applications",
  "keywords": [
    "symfony-ux",
    "symfony",
    "bpmn",
    "bpmn-js"
  ],
  "homepage": "https://www.scope.li",
  "license": "MIT",
  "authors": [
    {
      "name": "Scope GmbH",
      "homepage": "https://www.scope.li"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Scopeli\\UxBpmn\\": ""
    }
  },
  "require": {
    "php": ">=7.4.5",
    "ext-json": "*",
    "symfony/config": "^4.4.17|^5.0|^6.0",
    "symfony/dependency-injection": "^4.4.17|^5.0|^6.0",
    "symfony/http-kernel": "^4.4.17|^5.0|^6.0",
    "symfony/twig-bundle": "^4.4.17|^5.0|^6.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.4",
    "phpstan/phpstan": "^1.3",
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^4.4.17|^5.0|^6.0",
    "symfony/phpunit-bridge": "^5.2|^6.0",
    "symfony/var-dumper": "^4.4.17|^5.0|^6.0",
    "symfony/webpack-encore-bundle": "^1.11"
  },
  "conflict": {
    "symfony/flex": "<1.13",
    "symfony/webpack-encore-bundle": "<1.11"
  },
  "scripts": {
    "phpstan": [
      "php vendor/bin/phpstan"
    ],
    "phpunit": [
      "php vendor/bin/phpunit"
    ],
    "phpunit-coverage": [
      "XDEBUG_MODE=coverage php vendor/bin/phpunit"
    ],
    "test": [
      "@phpstan",
      "@phpunit"
    ],
    "php-cs-fixer": [
      " php vendor/bin/php-cs-fixer fix"
    ]
  }
}
